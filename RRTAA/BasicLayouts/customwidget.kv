<CustButton@Button>:
    font_size: 32
    color: 0,0,0,1
    size: 150, 50
    background_normal: '' # White button background
    background_down: 'bg.png' # Can change to png
    background_color: 0.88, 0.88, 0.88, 1
    size_hint: .4, .3

<CustomWidget>:
    CustButton:
        text: "Random"
        pos: root.x, root.y
    CustButton:
        text: "Random"
        pos: 200, 400
